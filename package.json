    "build": "babel src -d dist --source-maps"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "ejs": "2.7.1",
    "express": "4.19.2",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "1.16.2",
    "method-override": "3.0.0",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-geocaching": "0.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "nodemon": "^1.19.2"
  }
}
